<Router AppAssembly="@typeof(App).Assembly">
  <Found Context="routeData">
    <RouteView RouteData="@routeData" DefaultLayout="@typeof(SGINVENTARIO.Components.Layouts.MainLayout)" />
    <FocusOnNavigate RouteData="@routeData" Selector="h1" />
  </Found>
  <NotFound>
    <LayoutView Layout="@typeof(SGINVENTARIO.Components.Layouts.MainLayout)">
      <SGINVENTARIO.Components.Pages.NotFound />
    </LayoutView>
  </NotFound>
</Router>
